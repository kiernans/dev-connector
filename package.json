{
  "name": "dev-connector",
  "version": "1.0.0",
  "description": "Social network for developers",
  "main": "server.js",
  "scripts": {
    "start": "node server",
    "prodstart": "NODE_ENV=production node server.js",
    "server": "nodemon server",
    "client": "npm start --prefix client",
    "prodclient": "npm run prodstart --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "prod": "concurrently \"npm run server\" \"npm run prodclient\"",
    "render-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kiernans/dev-connector.git"
  },
  "author": "Kiernan Soriano",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kiernans/dev-connector/issues"
  },
  "homepage": "https://github.com/kiernans/dev-connector#readme",
  "dependencies": {
    "axios": "^0.26.1",
    "bcryptjs": "^2.4.3",
    "config": "^3.3.7",
    "express": "^4.17.3",
    "express-validator": "^6.14.0",
    "gravatar": "^1.8.2",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.2.6"
  },
  "devDependencies": {
    "concurrently": "^7.0.0",
    "nodemon": "^2.0.15"
  }
}
